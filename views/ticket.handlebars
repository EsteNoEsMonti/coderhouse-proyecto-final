<main>
  <h2 id="ticket-heading">{{Head}}</h2>
  <div>
    <h3>Thanks for your purchase {{ticket.purcheaser}}!</h3>

    <div>
      <h4>Sumary:</h4>
      {{#each list}}
      <div>
        <div>{{this.product.title}} [{{this.quantity}}x${{this.product.price}}] = ${{multiply
          this.product.price
          this.quantity
          }}</div>
        <p>-----------------------------------------------------------|</p>
      </div>
      {{/each}}
    </div>
    <h3>TOTAL: ${{ticket.amount}}</h3>
  </div>
</main>