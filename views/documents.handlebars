<div>
  <h3>{{Head}}</h3>
  <br />
  <form class="{{uid}}" action="/upload" method="POST" id="formDocs" enctype="multipart/form-data">
    <div>
      <label for="image-upload-identificator" id="label-identificator">
        Identificator File
      </label>
      <input type="file" id="image-upload-identificator" name="identificator" accept="image/*" multiple="false"
        onchange="handleFileChange()" />
    </div>
    <div>
      <label for="image-upload-address" id="label-address">
        Address File
      </label>
      <input type="file" id="image-upload-address" name="address" accept="image/*" multiple="false"
        onchange="handleFileChange()" />
    </div>
    <div>
      <label for="image-upload-status" id="label-status">
        Status File
      </label>
      <input type="file" id="image-upload-status" name="status" accept="image/*" multiple="false"
        onchange="handleFileChange()" />
    </div>
    <br /><br />
    <button type="submit" id="submit-btn" disabled="true">Submit</button>
  </form>
</div>

<script src="../public/js/documents.js" type="application/javascript"></script>